<%- include("partials/header") %> 

<style>
    .results-table{
        width:70%;
        margin:auto;
    }
</style>

<section id="events">
    <h1 class="tests"><%= event %> </h1>
</section>

<table class="table table-dark table-striped table-hover results-table">
    <thead>
      <tr>
        <th scope="col">#</th>
        <th scope="col">Name</th>
        <th scope="col">Email</th>
        <th scope="col">Score</th>
      </tr>
    </thead>
    <tbody>
        <% results.forEach(function(result,index){ %> 
            <tr>
              <th scope="row"> <%= index+1 %>  </th>
              <td> <%= result.name %>  </td>
              <td> <%= result.email %> </td>
              <td> <%= result.score %> </td>
            </tr>
        <% }) %> 
    </tbody>
  </table>

<%- include ("partials/footer") %> 