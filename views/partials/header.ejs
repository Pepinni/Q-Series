<!doctype html>
<html lang="en">

<head>
    <!-- Required meta tags -->
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <link href="https://fonts.googleapis.com/css?family=Roboto:300,400&display=swap" rel="stylesheet">

    <!-- Bootstrap CSS -->
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/css/bootstrap.min.css" rel="stylesheet"
        integrity="sha384-EVSTQN3/azprG1Anm3QDgpJLIm9Nao0Yz1ztcQTwFspd3yD65VohhpuuCOmLASjC" crossorigin="anonymous">

    <!-- Style for account admin and quiz-->
    <link rel="stylesheet" href="css/main.css">

    <!-- Style for only quiz -->
    <link rel="stylesheet" href="css/quiz.css">


    <!-- Font awesome -->
    <script src="https://kit.fontawesome.com/0464853cff.js" crossorigin="anonymous"></script>

    <!-- Google Fonts -->
    <link href='https://fonts.googleapis.com/css?family=Orbitron' rel='stylesheet' type='text/css'>
    <style>
        @import url('https://fonts.googleapis.com/css2?family=Dongle:wght@700&display=swap');
        @import url('https://fonts.googleapis.com/css2?family=Dongle:wght@700&display=swap');
        @import url('https://fonts.googleapis.com/css2?family=Slabo+27px&display=swap');
        @import url('https://fonts.googleapis.com/css2?family=Josefin+Slab:wght@700&display=swap');
        @import url('https://fonts.googleapis.com/css2?family=Oswald:wght@300&display=swap');
    </style>
    <title>
        <%=name%>
    </title>
</head>

<body>
    <section id="title">
        <div class="container-fluid">
            
            <!-- Nav Bar -->
            <nav class="navbar navbar-expand-lg navbar-fixed-top navbar-light">
                <a class="navbar-brand"><img src="images/logo3.png" class=" navbar-logo"></a>
                <button class="navbar-toggler" type="button" data-bs-toggle="collapse"
                    data-bs-target="#navbarSupportedContent" aria-controls="navbarSupportedContent"
                    aria-expanded="false" aria-label="Toggle navigation">
                    <span class="navbar-toggler-icon"></span>
                </button>
                <div class="collapse navbar-collapse" id="navbarSupportedContent">
                    <ul class="navbar-nav ms-auto">
                        <li class="nav-item">
                            <img src="<%=user.pic%>" class="user-pic">
                        </li>
                        <li class="nav-item">
                            <a class="nav-link" href="/logout"><i class="fas fa-sign-out-alt logout"></i></a>
                        </li>
                    </ul>
                </div>
            </nav>
        </div>
    </section>